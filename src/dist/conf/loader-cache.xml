<?xml version='1.0' encoding='UTF-8'?>
<ehcache>
	<diskStore path="java.io.tmpdir" />

	<defaultCache maxElementsInMemory="5000" eternal="true" timeToIdleSeconds="0"
		timeToLiveSeconds="0" overflowToDisk="true">
	</defaultCache>

	<cache name="PathCache" maxElementsInMemory="100000" eternal="true" overflowToDisk="true"
		diskPersistent="true" />

	<cache name="RandomUploadCache" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" diskPersistent="true" />
</ehcache>